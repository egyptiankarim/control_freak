<tr data-link="<%= control_path(control.number) %>" class="<%= row_class(control) %>">
  <td scope="row">
    <%= control[:number] %>
  </td>
  <td><%= control[:title] %></td>
  <td class="text-center"><%= control[:priority] %></td>
  <% if @row_class == "control" || @row_class == "enhancement" %>
  <td class="text-center">
    <% if control.is_baseline_impact_low %>
    <i class='fa fa-check-circle'></i>
    <% else %>
    <i class="fa fa-times-circle"
    <% end %>
  </td>
  <td class="text-center">
    <% if control.is_baseline_impact_moderate %>
    <i class='fa fa-check-circle'></i>
    <% else %>
    <i class="fa fa-times-circle"
    <% end %>
  </td>
  <td class="text-center">
    <% if control.is_baseline_impact_high %>
    <i class='fa fa-check-circle'></i>
    <% else %>
    <i class="fa fa-times-circle"
    <% end %>
  </td>
  <% elsif @row_class == "not-selected" %>
  <td colspan="3" class="text-center">Not Selected</td>
  <% elsif @row_class == "withdrawn" %>
  <td colspan="3" class="text-center">Withdrawn</td>
  <% else %>
  <td colspan="3" class="text-center"></td>
  <% end %>
</tr>
